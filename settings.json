        "gws_omix:uniprot_fungi_tab_url": "https://storage.gra.cloud.ovh.net/v1/AUTH_a0286631d7b24afba3f3cdebed2992aa/opendata/omix/db/uniprot-taxonomy_v1_4751.tab.gz",
        "gws_omix:uniprot_fungi_tab_file": "/data/gws_omix/opendata/uniprot-taxonomy_v1_4751.tab.gz"
    },
    "environment":{
        "pip":[
            {
                "source": "https://pypi.python.org/simple",
                "packages":[]
            },
            {
                "source": "https://$GWS_PIP_PACKAGE_TOKEN@gitlab.com/api/.../packages/pypi",
                "packages":[]
            }
        ],
        "git":[
            {
                "source": "https://$GWS_GIT_LOGIN:$GWS_GIT_PWD@gitlab.com/gencovery/core",
                "packages":[
                    {"name": "gws_core", "version":"0.3.16", "commit": "", "branch": "", "is_brick": true}
                ]
            },
            {
                "source": "https://$GWS_GIT_LOGIN:$GWS_GIT_PWD@gitlab.com/gencovery/lib",
                "packages":[
                    {"name": "deepec", "is_brick": false}
                ]
            }
        ],
        "variables":{
            "GWS_GIT_LOGIN": "",
            "GWS_GIT_PWD": "",
            "GWS_PIP_PACKAGE_TOKEN": ""
        }
    }
}